#include <stdio.h>
void main(){
    int i, n;
    printf("Enter the size of the array : ");
    scanf("%d", &n);
    int arr[n];
    
    printf("\nEnter the elements one by one :\n");
    for(i=0; i<n; i++){
        scanf("%d", &arr[i]);
    }
    printf("\nThe entered array is :\n");
    for(i=0; i<n; i++){
        printf("%d\t", arr[i]);
    }
}
